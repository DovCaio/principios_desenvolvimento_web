{
  "name": "pdweb",
  "version": "1.0.0",
  "description": "Laboratório - Criação de repositório, definição do projeto e HTML com funcionalidade principal  Pré-requisitos: node.js, express e Javascript.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test:watch": "jest --watch",
    "test": "jest --runInBand --coverage",
    "test:resident": "jest --runInBand --testPathPatterns=tests/resident.e2e.test.ts",
    "test:employee": "jest --runInBand --testPathPatterns=tests/employee.e2e.test.ts",
    "test:lot": "jest --runInBand --testPathPatterns=tests/lot.e2e.test.ts",
    "test:service": "jest --runInBand --testPathPatterns=tests/serviceRequest.e2e.test.ts",
    "test:visitor": "jest --runInBand --testPathPatterns=tests/visitant.e2e.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DovCaio/minitest1_principios_desenvovolvimento_web.git"
  },
  "keywords": [],
